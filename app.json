{
  "expo": {
    "userInterfaceStyle": "automatic",
    "name": "Chacara Guaruja",
    "slug": "CG",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/android/drawable-xxxhdpi/icon.png",
    "splash": {
      "image": "./assets/android/drawable-xxxhdpi/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#F0FFF0"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.iris.chacaraguaruja",
      "buildNumber": "1.0.0"
    },
    "android": {
      "package": "com.iris.chacaraguaruja",
      "googleServicesFile": "./google-services.json",
      "permissions": [
        "CAMERA",
        "WRITE_EXTERNAL_STORAGE",
        "CAMERA_ROLL",
        "USER_FACING_NOTIFICATIONS "
      ],
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/android/drawable-xxxhdpi/ic_launcher-web.png",
        "backgroundColor": "#FFFFFF"
      }
    }
  }
}
